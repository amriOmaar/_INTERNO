{% extends 'basefront.html.twig' %}

{% block title %}Stagiaire{% endblock %}

{% block body %}
    <h1>Stagiaire</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdStagiaire</th>
                <td>{{ stagiaire.idStagiaire }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ stagiaire.nom }}</td>
            </tr>
            <tr>
                <th>Mail</th>
                <td>{{ stagiaire.mail }}</td>
            </tr>
            <tr>
                <th>Mdp</th>
                <td>{{ stagiaire.mdp }}</td>
            </tr>
            <tr>
                <th>Faculte</th>
                <td>{{ stagiaire.faculte }}</td>
            </tr>
            <tr>
                <th>NumTel</th>
                <td>{{ stagiaire.numTel }}</td>
            </tr>
            <tr>
                <th>Stage</th>
                <td>{{ stagiaire.stage }}</td>
            </tr>
            <tr>
                <th>DateDebut</th>
                <td>{{ stagiaire.dateDebut ? stagiaire.dateDebut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>DateFin</th>
                <td>{{ stagiaire.dateFin ? stagiaire.dateFin|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ stagiaire.image }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_stagiaire_index') }}">back to list</a>

    <a href="{{ path('app_stagiaire_edit', {'idStagiaire': stagiaire.idStagiaire}) }}">edit</a>

    {{ include('stagiaire/_delete_form.html.twig') }}
{% endblock %}
