                            <ul class="block-images row">
                                <li class="col-md-6 col-sm-6"><img src="{{ asset('front/img/5.jpg') }}" alt=""> </li>
                                <li class="col-md-6 col-sm-6"><img src="{{ asset('front/img/6.jpg') }}" alt=""> </li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-5 offset-md-1  ">
